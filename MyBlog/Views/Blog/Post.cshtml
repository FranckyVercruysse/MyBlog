@model MyBlog.Models.Post

<div id="content">
    <div class="row">
        <div class="col-md-8">
            <article>
                <h2>
                    <a asp-controller="Blog"
                       asp-action="Post"
                       asp-route-year="@Model.PostedOn.Year"
                       asp-route-month="@Model.PostedOn.Month"
                       asp-route-title="@Model.UrlSlug"
                       title="@Model.Title">
                        @Model.Title
                    </a>
                </h2>
                <div class="row mb-5">
                    <div class="col-sm-6 col-md-6">
                        <i class="fa fa-folder-open" aria-hidden="true"></i>
                        <a asp-controller="Blog"
                           asp-action="Category"
                           asp-route-category="@Model.Category.UrlSlug"
                           title="@Model.Category.Name">
                            @Model.Category.Name
                        </a>
                        <!--tag list -->
                        <i class="fa fa-tag" aria-hidden="true"></i>
                        <tag-list class="post_tags" post-tags="@Model.PostTags"></tag-list>
                    </div>
                    <div class="col-sm-6 col-md-6">
                        <i class="fa fa-comment-o" aria-hidden="true"></i><a href="#">20 Comments</a>
                        <i class="fa fa-clock-o" aria-hidden="true"></i>@Model.PostedOn.ToString("dd/M/yyyy")
                    </div>
                </div>

                <!--Short Description-->
                <div class="post-body">
                    @Html.Raw(Model.Description)
                </div>
            </article>

            <div class="card card-outline-secondary text-center mb-5" style="background-color: #eceeef;">
                <div class="card-body">
                    <h4 class="card-title">Leave a comment</h4>
                    <form role="form" class="row">
                        <div class="col-md-6 form-group">
                            <label class="sr-only" for="name">Name</label>
                            <input type="text" class="form-control" id="name" placeholder="Your Name">
                        </div>
                        <div class="col-md-6 form-group">
                            <label class="sr-only" for="email">Email address</label>
                            <input type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Enter email">
                            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
                        </div>

                        <div class="col-md-12 form-group">
                            <label class="sr-only" for="textarea">Example textarea</label>
                            <textarea class="form-control" id="textarea" rows="3"></textarea>
                        </div>

                        <div class="col-md-12 form-group text-right">
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                    </form>
                </div>
            </div>


            <ul id="comments" class="comments">
                <li class="comment">
                    <div class="clearfix">
                        <h4 class="float-left">John</h4>
                        <p class="float-right">9:41 PM on August 24, 2017</p>
                    </div>
                    <p>
                        <em>
                            Hello Francky,<br /><br />

                            thank you for your fast reply.

                            I already added 2 Breakpoints - see screenshot - but the function is not called.
                            I think I miss some implementation in the first place.
                            There is no JS error according to Firefox and Chrome.
                        </em><br />
                        <img alt="screenshot" class="img-thumbnail img-fluid" src="data:image/jpeg;base64," />




                    </p>
                </li>

                <li class="comment clearfix">
                    <div class="clearfix">
                        <h4 class="pull-left">John</h4>
                        <p class="pull-right">9:41 PM on August 24, 2017</p>
                    </div>
                    <p>
                        <em>I installed VS 2013 Community and it runs perfectly. The only problem is that in VS Express there's no warning or explanation why tests don't run, so you can waste a lot of time. BTW - this a great series of tutorials which I am really enjoying.</em>
                    </p>
                </li>
            </ul>


        </div>

        <div class="col-md-4">
            <!-- Sidebar -->
            @await Component.InvokeAsync("Sidebar")
        </div>
    </div>
</div>